{
  "name": "baileys-redis-auth",
  "version": "1.1.0",
  "description": "Redis Auth for Baileys",
  "author": "heriyanto binduni <hbinduni@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/hbinduni/baileys-redis-auth",
  "repository": {
    "url": "https://github.com/hbinduni/baileys-redis-auth.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "bun run clean && tsc -p tsconfig.json",
    "prebuild": "bun run check:all",
    "example": "ts-node -r tsconfig-paths/register example/example.ts",
    "example:no-all": "bun run example -- --no-store --no-reply",
    "typecheck": "bun x tsc --noEmit -p tsconfig.json",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:all": "bun run typecheck && bun run check",
    "check:all:fix": "bun run typecheck && bun run check:fix"
  },
  "files": [
    "lib",
    "README.md",
    "LOGOUT_GUIDE.md",
    "LICENSE"
  ],
  "keywords": [
    "baileys",
    "redis",
    "auth"
  ],
  "devDependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@biomejs/biome": "^1.9.4",
    "@hapi/boom": "^10.0.1",
    "@types/node": "^24.7.0",
    "dotenv": "^17.2.3",
    "node-cache": "^5.1.2",
    "pino": "^9.13.1",
    "pino-pretty": "^13.1.1",
    "qrcode-terminal": "^0.12.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.20",
    "ioredis": "^5.8.1"
  }
}
